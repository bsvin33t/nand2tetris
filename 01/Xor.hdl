/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandAB);
    Nand(a=a, b=nandAB, out=nandAnandAB);
    Nand(a=nandAB, b=b, out=nandABnandB);
    Nand(a=nandAnandAB, b=nandABnandB, out=out);
}